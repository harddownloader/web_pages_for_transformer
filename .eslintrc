{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "curly":"off",
        "camelcase":"off",
        "no-var":"off",
        "no-trailing-spaces":"off",
        "max-len":"off",
        "new-cap":"off",
        "one-var":"off"
    },
    "extends": ["eslint:recommended", "google"],
}